	</cmd>
    <cmd id="101" idtxt="MODULE_CMD_DELETE_CHANNEL" cmd="deletechn" desc="Delete module channel." access="OSA_CMD_ACCESS_USER">
		<arg type="OSA_CMD_ARG_UNSIGNED" tag="c" name="Channel ID (0-15)" required="1" />
	</cmd>
    <cmd id="102" idtxt="MODULE_CMD_SET_BITRATE" cmd="setbitrate" desc="Set module channel bit rate." access="OSA_CMD_ACCESS_USER">
		<arg type="OSA_CMD_ARG_UNSIGNED" tag="c" name="Channel ID (0-15)" required="1" />
